    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.5.3"
  },
  "elf": {
    "cli": {
      "repoTemplate": "class",
      "plugins": [
        "@ngneat/elf-cli-ng"
      ]
    }
