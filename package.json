{
  "name": "done-list",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "fix": "eslint --cache --fix src/ && prettier --write src/",
    "i18n": "ng extract-i18n --format=json --output-path src/locale",
